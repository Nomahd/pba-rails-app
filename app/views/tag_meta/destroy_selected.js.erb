<% unless params[:selected_genre].nil? %>
  var rows = <%= raw params[:selected_genre] %>
<% end %>
<% unless params[:selected_theme].nil? %>
  var rows = <%= raw params[:selected_theme] %>
<% end %>
<% unless params[:selected_special].nil? %>
  var rows = <%= raw params[:selected_special] %>
<% end %>

rows.forEach(function(id) {
    var row = document.querySelector("tr[id='" + id + "']")
    row.parentNode.removeChild(row)
})

